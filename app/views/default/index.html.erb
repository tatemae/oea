<% @items.each do |id, item| -%>
  <div>
    <div class="question_text">
      <% item.css('presentation/material/mattext').each do |question_text| -%>
        <%= item_content(question_text) %>
      <% end -%>
    </div>
    <div class="answers">
      <% item.css('response_lid/render_choice/response_label').each do |answer_text| -%>
        <div class="answer_text">
          <% answer_id = answer_text.first[1] -%>
          <input type="radio" name="<%= id %>" value="<%= answer_id %>"><%= sanitize(item_content(answer_text.css('mattext')[0])) %>
        </div>
      <% end -%>
    </div>
  </div>
  <div class="embed_code">
    <%= CGI.unescapeHTML("<iframe width=\"300\" height=\"300\" src=\"#{request.protocol}#{request.host_with_port}#{item_path(id)}\"></iframe>") %>
  </div>
  <br/>
<% end -%>
